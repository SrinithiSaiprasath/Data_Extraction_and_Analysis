    "import xarray as xr\n",
    "import os\n",
    "import numpy as np\n",
    "from datetime import datetime\n",
    "\n",
    "file_path = "cannot be mentioned here(confidential)",
    "ds = xr.open_dataset(file_path)\n",
    "\n",
    "unique_latitudes = ds['latitude'].values\n",
    "unique_longitudes = ds['longitude'].values\n",
    "unique_pressure_levels = ds['level'].values \n",
    "unique_time = ds['time'].values.flatten()\n",
    "\n",
    "lat1 = 25.0\n",
    "lon1 = 65.0\n",
    "date1 = \"2011-01-05\"\n",
    "level = 500\n",
    "\n",
    "output_directory = f\"data_{lat1}_{lon1}_{date1}\"\n",
    "os.makedirs(output_directory , exist_ok = True)\n",
    "print(f\"Data for Latitude:{lat1} Longitude:{lon1} on Date:{date1} on Level:500 mb \")\n",
    "print(\"Date\\t\\tTime\\t\\tTemperature(K)\\t\\tTemperature(deg C)\")\n",
    "for lat in unique_latitudes:\n",
    "    for lon in unique_longitudes:\n",
    "        for time1 in unique_time:\n",
    "            data_for_params = ds.sel(latitude=lat, longitude=lon, time = time1,  method='nearest')\n",
    "            time_data_array = str(np.array(time1, dtype='datetime64'))\n",
    "            date,time1 = time_data_array.split(\"T\")\n",
    "            time2,unn = time1.split(\".\")\n",
    "            time = time2.replace(\":\",\"-\")\n",
    "            if(lat==lat1 and lon==lon1 and date==date1):\n",
    "                temperature_data = data_for_params['t']\n",
    "                pressure_data = data_for_params['level']\n",
    "\n",
